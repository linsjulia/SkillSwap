{
  "extends": "expo/tsconfig.base.json",
  "compilerOptions": {
    "jsx": "react",
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ],
      "@firebase/auth": [
        "./node_modules/@firebase/auth/dist/index.rn.d.ts"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "index.js",
    "src/app/index.tsx",
    "src/services/verificarColecaoUser.js",
    "src/app/(stack)/settings.js",
    "src/services/verificarColecaoUser.tsx",
    "src/components/ProfileImage.js",
    "src/app/(stack)/curriculum.tsxx",
    "src/components/candidatosList.tsx",
    "src/components/Slider.tsx",
    "nativewind-env.d.ts"
, "src/services/githubAuthConfig.js"  ]
}