{
  "expo": {
    "name": "SkillSwap",
    "slug": "SkillSwap",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logoSkillSwap.png",
    "scheme": "SkillSwap",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/logoSkillSwap.png",
      "resizeMode": "contain",
      "Width ": 30,
      "backgroundColor": "black"
    },
    "ios": {
      "supportsTablet": false
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logoSkillSwap.png",
        "backgroundColor": "#ffffff"
        
      },
      "package": "com.skillswap.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Inter.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "b3a5d3e0-4d34-4de3-a48f-6d28283eb586"
      }
    }
  }
}
