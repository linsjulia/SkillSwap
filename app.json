{
  "expo": {
    "name": "SkillSwap",
    "slug": "SkillSwap",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logoSkillSwap.png",
    "scheme": "SkillSwap",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/logoSkillSwap.png",
      "resizeMode": "contain",
      "Width ": 30,
      "backgroundColor": "black"
    },
    "ios": {
      "supportsTablet": false
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logoSkillSwap.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [ "expo-router", ["expo-font", { "fonts": [ "./assets/fonts/Inter.ttf" ] } ]],
    
    "experiments": {
      "typedRoutes": true
    }
  }
}
